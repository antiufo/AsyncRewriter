{
  "version": "0.8.6",
  "packOptions": {
    "owners": [ "Shay Rojansky" ],
    "authors": [ "Shay Rojansky" ],
    "description": "Uses Roslyn to rewrite synchronous C# methods into async counterparts",
    "repository": {
      "type": "git",
      "url": "git://github.com/roji/AsyncRewriter"
    }
  },
  "buildOptions": {
    "embed": {
      "includeFiles": "AsyncRewriterHelpers.cs"
    }
  },
  "dependencies": {
    "Microsoft.CodeAnalysis.CSharp": "1.3.0"
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Reflection": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Text.Encoding": "4.0.0.0",
        "System.Threading.Tasks": "4.0.0.0",
        "Microsoft.Build.Framework": "4.0.0.0",
        "Microsoft.Build.Utilities.v4.0": "4.0.0.0"
      }
    },
    "netstandard1.5": {
      "dependencies": {
        "System.Collections.Immutable": "1.2.0",
        "System.Console": "4.0.0",
        "System.Data.Common": "4.1.0",
        "System.IO.FileSystem": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Reflection": "4.1.0",
        "System.Reflection.Extensions": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Text.Encoding.Extensions": "4.0.11"
      }
    }
  }
}
